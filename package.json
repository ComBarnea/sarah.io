  "devDependencies": {
    "@types/lodash": "^4.14.80",
    "@types/node": "^7.0.5",
    "@types/npm": "^2.0.28",
    "@types/reflect-metadata": "0.0.5",
    "nodemon": "^1.12.1",
    "reflect-metadata": "^0.1.10",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3",
